@model IEnumerable<SchoolProject.Web.Data.Entities.OtherEntities.Gender>

@{
    ViewData[index: "Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
    <tr>
        <th>
            @Html.DisplayNameFor(expression: model => model.Name)
        </th>
        <th>
            @Html.DisplayNameFor(expression: model => model.ProfilePhotoId)
        </th>
        <th>
            @Html.DisplayNameFor(expression: model => model.IdGuid)
        </th>
        <th>
            @Html.DisplayNameFor(expression: model => model.WasDeleted)
        </th>
        <th>
            @Html.DisplayNameFor(expression: model => model.CreatedAt)
        </th>
        <th>
            @Html.DisplayNameFor(expression: model => model.UpdatedAt)
        </th>
        <th></th>
    </tr>
    </thead>
    <tbody>
    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(expression: modelItem => item.Name)
            </td>
            <td>
                @Html.DisplayFor(expression: modelItem => item.ProfilePhotoId)
            </td>
            <td>
                @Html.DisplayFor(expression: modelItem => item.IdGuid)
            </td>
            <td>
                @Html.DisplayFor(expression: modelItem => item.WasDeleted)
            </td>
            <td>
                @Html.DisplayFor(expression: modelItem => item.CreatedAt)
            </td>
            <td>
                @Html.DisplayFor(expression: modelItem => item.UpdatedAt)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.Id">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.Id">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.Id">Delete</a>
            </td>
        </tr>
    }
    </tbody>
</table>